{
  "name": "IoT-Group-India",
  "version": "1.0.0",
  "description": "AU IoT Milestone 2",
  "main": "build/server.js",
  "scripts": {
    "views": "cp -r ./src/views/ ./build/views/",
    "resources": "cp ./src/resources/resources.json ./build/resources/resources.json",
    "dev": "ts-node ./src/server.ts",
    "nodemon": "npm run copy && nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "tsc": "tsc",
    "start": "node build/server.js",
    "build": "npm run views && npm run resources && npm run tsc",
    "copy": "npm run views && npm run resources",
    "buildstart": "npm run build && npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "node-dht-sensor": "0.0.34",
    "nodemon": "^1.18.4",
    "onoff": "^3.2.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/es6-shim": "^0.31.37",
    "@types/express": "^4.16.0",
    "@types/node": "^10.9.4",
    "@types/onoff": "0.0.28",
    "@types/ws": "^6.0.1"
  }
}
