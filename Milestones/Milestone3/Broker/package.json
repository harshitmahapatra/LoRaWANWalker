{
  "name": "IoT-Group-India",
  "version": "1.0.0",
  "description": "AU IoT Milestone 3",
  "main": "build/server.js",
  "scripts": {
    "views": "cp -r ./src/views/ ./build/views/",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "dev": "ts-node ./src/server.ts",
    "tsc": "tsc",
    "start": "node build/server.js",
    "build": "npm run tsc && npm run views",
    "buildstart": "npm run tsc && npm run views && npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "mosca": "^2.8.3",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/mosca": "^2.8.1",
    "@types/node": "^10.10.1"
  }
}
