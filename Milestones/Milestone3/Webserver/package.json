{
  "name": "IoT-Group-India",
  "version": "1.0.0",
  "description": "AU IoT Milestone 3",
  "main": "build/server.js",
  "scripts": {
    "views": "cp -r ./src/views/ ./build",
    "dev": "npm run views && ts-node ./src/server.ts",
    "nodemon": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "tsc": "tsc",
    "start": "node build/server.js",
    "build": "rm -r ./build && npm run tsc && npm run views",
    "buildstart": "npm run build && npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "mqtt": "^2.18.8",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/ws": "^6.0.1",
    "@types/express": "^4.16.0",
    "@types/node": "^10.10.1"
  }
}
